# ZTimeHelp Example Configuration
#
# Copy this file to ~/.ztimehelp/config.env and edit with your personal tokens

# GitHub Configuration
GITHUB_TOKEN=your_personal_access_token_here
GITHUB_USERNAME=your_github_username
GITHUB_ORGANIZATION=optional_organization_name

# Zoom Configuration
ZOOM_API_KEY=your_zoom_api_key
ZOOM_API_SECRET=your_zoom_api_secret
ZOOM_USER_ID=me

# Slack Configuration
SLACK_TOKEN=xoxb-your-slack_bot_token_here
